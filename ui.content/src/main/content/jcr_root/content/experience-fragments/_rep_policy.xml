<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:crx="http://www.day.com/crx/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="experience-fragments-service"
        rep:privileges="{Name}[jcr:read,rep:write]"/>
    <deny1
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[rep:privilegeManagement,jcr:versionManagement,jcr:namespaceManagement,jcr:modifyAccessControl,jcr:lifecycleManagement,jcr:readAccessControl,rep:indexDefinitionManagement,jcr:nodeTypeDefinitionManagement,rep:write,crx:replicate,jcr:retentionManagement,jcr:lockManagement,rep:userManagement,jcr:workspaceManagement]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="experience-fragments-editors"
        rep:privileges="{Name}[jcr:versionManagement,rep:write,crx:replicate,jcr:lockManagement]"/>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:read]"/>
</jcr:root>
